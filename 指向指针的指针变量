void main()
{char *name[]={"Follow me","BASIC","Great Wall","FORTRAN","Computer desighn"};
 char **p;
 int i;
 for(i=0;i<5;i++)
   {p=name+i;
    printf("%s\n",*p);
   }
}


void main()
{static int a[5]={1,3,5,7,9};
 int *num[5]={&a[0],&a[1],&a[2],&a[3],&a[4]};
 int **p,i;
 p=num;
 for(i=0;i<5;i++)
   {printf("%d\t",**p);p++;}
}
