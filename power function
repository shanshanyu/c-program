void main(){
 int power(int base,int n);
printf(“%d %d\n”,power(2,3),power(2,5));
}
int power(int base,int n){
  int i; /*循环变量*/
 int p=1;/*保存sum的值*/
  for(i=1;i<=n;i++) p=p*base;  /*point*/
  return p;
}






int power(int base,int n){    /*n与0比较*/
  int p;
  for(p=1;n>0;n--)p=p*base;
  return p;
}


int power(int base,int n){
  int p=1;
  int i;
  for(i=1;i<=n;i++) p=p*base;  /*n与i比较*/
}


