int strcmp(const char *strDes,const char *strSrc){
  int ret=0;
  while(!(ret=*(unsigned char *)strDes-*(unsigned char *)strSrc)&&*strDes){  //循环条件：两个数相等或其中且任意一个不等于'\0'
    strDes++;                                                                //unsigned char 为了防止出现一些不常见字符，ASCII值大于127
    strSrc++;
  }
  if(ret>0) return 1;
  else if(ret<0) return -1;
  return ret;
}
